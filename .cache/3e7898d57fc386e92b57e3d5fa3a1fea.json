  smoothScrollTo(0, window.scrollY - window.innerHeight, 225)\n    },\n  }\n\n  componentWillMount() {\n    console.log(\"name is\", this.props.name)\n    this.store = new Store(this.props.name)\n    window._store = this.store\n  }\n\n  show() {\n    const { book } = this.store\n\n    if (!book) return <loading>loading</loading>\n\n    return (\n      <book style={{ padding: 10 }}>\n        <Toolbar store={this.store} />\n        <HotKeys keyMap={map}>\n          <mainArea>\n            <Top store={this.store} />\n            <content>\n              <HotKeys handlers={this.handlers}>\n                <TOC store={this.store} />\n                {book.chapters.map(html => (\n                  <Chapter store={this.store} html={html} />\n                ))}\n              </HotKeys>\n            </content>\n            {false && <Status />}\n          </mainArea>\n        </HotKeys>\n      </book>\n    )\n  }\n\n  styles = {\n    content: {\n      margin: 30,\n      maxWidth: 800,\n      alignSelf: \"center\",\n    },\n    book: {\n      display: \"block\",\n      background: \"white\",\n    },\n\n    loading: {\n      fontSize: 32,\n      opacity: 0.6,\n      marginTop: 200,\n    },\n  }\n}\n"},"lineCount":null}},"hash":"7a5b499e1cb87abefefef3a639707e24","cacheData":{"env":{}}}