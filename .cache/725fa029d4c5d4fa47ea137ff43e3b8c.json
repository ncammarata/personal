}},{"generated":{"line":148,"column":0},"source":"app/views/book.js","original":{"line":68,"column":3}},{"name":"Component","generated":{"line":152,"column":2},"source":"app/views/book.js","original":{"line":17,"column":34}},{"name":"Book","generated":{"line":154,"column":18},"source":"app/views/book.js","original":{"line":17,"column":21}}],"sources":{"app/views/book.js":"import { HotKeys } from \"react-hotkeys\"\nimport Focus from \"./focus\"\nimport { observer } from \"mobx-react\"\nimport Top from \"./top\"\nimport Toolbar from \"./toolbar\"\nimport { stop } from \"react-icons-kit/iconic/stop\"\nimport { Icon } from \"react-icons-kit\"\nimport { sample } from \"lodash\"\n\nconst map = {\n  nextPage: \"right\",\n  prevPage: \"left\",\n  pause: \"space\",\n}\n\n@observer\nexport default class Book extends Component {\n  handlers = {\n    nextPage: () => {\n      this.store.nextPage()\n    },\n    prevPage: () => {\n      this.store.prevPage()\n    },\n    space: () => {\n      this.store.stopPlaying()\n    },\n  }\n\n  show() {\n    const loadingMessages = [\n      `Words Coming Soon`,\n      `Relax, Read a Book`,\n      `Reticulating Splines`,\n      `Calculating AI`,\n    ]\n\n    return <Focus />\n\n    if (!book) {\n      return (\n        <loading>\n          <Top store={this.store} />\n          {sample(loadingMessages)}\n        </loading>\n      )\n    }\n\n    return (\n      <book>\n        {this.store.isPlaying && (\n          <stop onClick={this.store.stopPlaying}>\n            <Icon icon={stop} />\n          </stop>\n        )}\n        <Toolbar store={this.store} />\n        <HotKeys keyMap={map}>\n          <mainArea>\n            {!focusing && <Top store={this.store} />}\n            <HotKeys handlers={this.handlers}>\n              {focusing && <Focus store={this.store} />}\n              {!focusing && <content ref={this.renderContent} />}\n            </HotKeys>\n          </mainArea>\n        </HotKeys>\n      </book>\n    )\n  }\n\n  styles = {\n    content: {\n      display: \"block\",\n      height: \"auto !important\",\n      marginTop: 40,\n      height: \"580px\",\n      margin: \"40px auto 0px auto\",\n      width: 600,\n      overflow: \"visible\",\n    },\n    mainArea: {\n      height: \"100%\",\n    },\n    book: {\n      display: \"block\",\n      background: \"white\",\n    },\n    stop: {\n      position: \"fixed\",\n      right: 20,\n      bottom: 20,\n      padding: 10,\n      borderRadius: 5,\n      border: `1px solid rgba(0, 0, 0, 0.2)`,\n    },\n\n    loading: {\n      fontSize: 32,\n      flex: 1,\n      alignItems: \"center\",\n      justifyContent: \"center\",\n      opacity: 0.6,\n      marginTop: 200,\n    },\n  }\n}\n"},"lineCount":null}},"hash":"013838f5a9fdb246c35a99840cd1900d","cacheData":{"env":{}}}