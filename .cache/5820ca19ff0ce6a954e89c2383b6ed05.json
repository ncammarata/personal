ne":45,"column":21}}],"sources":{"app/views/plays.js":"import { Icon } from \"react-icons-kit\"\nimport { observer } from \"mobx-react\"\nimport { ic_play_circle_outline } from \"react-icons-kit/md/ic_play_circle_outline\"\n\n@observer\nclass Play extends Component {\n  state = { hover: false }\n\n  show() {\n    const { isDull, isActive, onClick } = this.props\n\n    return (\n      <play\n        className={isActive && \"active\"}\n        $hover={this.state.hover}\n        $isDull={isDull}\n        $isActive={isActive}\n        onMouseLeave={() => this.setState({ hover: false })}\n        onMouseEnter={() => this.setState({ hover: true })}\n        onClick={onClick}\n      >\n        <Icon size={24} icon={ic_play_circle_outline} />\n      </play>\n    )\n  }\n\n  styles = {\n    play: {\n      opacity: 0.4,\n      transition: \"opacity 80ms ease-in\",\n    },\n    isDull: {\n      opacity: 0.15,\n    },\n    isActive: {\n      opacity: 1,\n    },\n    hover: {\n      opacity: 1,\n    },\n  }\n}\n\n@observer\nexport default class Plays extends Component {\n  show() {\n    const { positions, cfiBase, texts, store, onPlay } = this.props\n\n    return (\n      <plays>\n        {positions.map(({ left, top, width, height }, index) => {\n          const styles = { top, width, height, left }\n          const isActive =\n            store.isPlaying !== false &&\n            store.isPlaying.cfiBase === cfiBase &&\n            store.isPlaying.index === index\n\n          return (\n            <play key={index} style={styles}>\n              <relative>\n                <overlay\n                  style={{ left, top, width, height }}\n                  $dullOverlay={store.isPlaying !== false && !isActive}\n                />\n                <icon>\n                  <Play\n                    isActive={isActive}\n                    isDull={store.isPlaying !== false && !isActive}\n                    onClick={() => {\n                      if (isActive) {\n                        store.stopPlaying()\n                      } else {\n                        onPlay(index)\n                      }\n                    }}\n                  />\n                </icon>\n              </relative>\n            </play>\n          )\n        })}\n      </plays>\n    )\n  }\n\n  styles = {\n    relative: {\n      position: \"relative\",\n    },\n    icon: {\n      position: \"absolute\",\n      left: -5,\n      top: 5,\n    },\n    play: {\n      position: \"absolute\",\n    },\n    overlay: {\n      pointerEvents: \"none\",\n      position: \"absolute\",\n      left: 0,\n      top: 0,\n      pointerEvents: \"none\",\n      transition: \"background 150ms ease-in\",\n      background: `rgba(255, 255, 255, 0)`,\n    },\n    dullOverlay: {\n      background: `rgba(255, 255, 255, 0.8)`,\n    },\n  }\n}\n"},"lineCount":null}},"hash":"a729cb769672f51636b793c24f7d516b","cacheData":{"env":{}}}