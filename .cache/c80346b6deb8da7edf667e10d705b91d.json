/minimap.js","original":{"line":8,"column":9}},{"generated":{"line":101,"column":0},"source":"app/views/minimap.js","original":{"line":7,"column":6}},{"generated":{"line":101,"column":7},"source":"app/views/minimap.js","original":{"line":6,"column":4}},{"generated":{"line":102,"column":0},"source":"app/views/minimap.js","original":{"line":31,"column":3}},{"name":"Component","generated":{"line":106,"column":2},"source":"app/views/minimap.js","original":{"line":2,"column":37}},{"name":"MiniMap","generated":{"line":108,"column":18},"source":"app/views/minimap.js","original":{"line":2,"column":21}}],"sources":{"app/views/minimap.js":"@observer\nexport default class MiniMap extends Component {\n  show() {\n    const { store } = this.props\n\n    return (\n      <minimap>\n        {store.texts.map((para, index) => {\n          const comments =\n            store.comments[store.commentsKey(store.activeChapter, index)] || []\n\n          return (\n            <para\n              key={store.activeChapter + \":\" + index}\n              $liked={includes(store.likes, index)}\n              $hasComments={comments.length > 0}\n              $active={index === store.activeIndex}\n              onMouseDown={() => store.setActive(index)}\n            >\n              {para\n                .split(\" \")\n                .slice(0, 20)\n                .map((word, index) => (\n                  <word key={index} style={{ width: word.length * 2 }} />\n                ))}\n            </para>\n          )\n        })}\n      </minimap>\n    )\n  }\n\n  styles = {\n    minimap: {\n      height: \"100%\",\n      width: 120,\n      overflow: \"scroll\",\n      borderLeft: `1px solid rgba(0, 0, 0, 0.1)`,\n      borderRight: `1px solid rgba(0, 0, 0, 0.1)`,\n      // borderRadius: 5,\n      paddingTop: 5,\n      paddingBottom: 5,\n      display: \"block\",\n    },\n    hasComments: {\n      background: \"#eaf5fb\",\n    },\n    para: {\n      flexFlow: \"row\",\n      position: \"relative\",\n      flexWrap: \"wrap\",\n      padding: `4px 6px`,\n      lineHeight: 1,\n      transition: \"background 300ms cubic-bezier(0.785, 0.135, 0.150, 0.860)\",\n      borderLeft: `1px solid rgba(0, 0, 0, 0)`,\n    },\n    active: {\n      borderLeft: `1px solid rgba(0, 0, 0, 0.7)`,\n    },\n    liked: {\n      background: \"rgba(234, 70, 60, 0.1)\",\n      borderLeft: `1px solid rgba(255,109,94, 1)`,\n    },\n    word: {\n      height: 2,\n      marginLeft: 1,\n      marginRight: 1,\n      marginTop: 1,\n      marginBottom: 3,\n      background: `rgba(0, 0, 0, 0.2)`,\n    },\n  }\n}\n"},"lineCount":null}},"hash":"040f8e5db0632b272d5952990faafb7b","cacheData":{"env":{}}}