nMouseDown={() => onSectionClick(index)}\n            >\n              {para\n                .split(\" \")\n                .slice(0, 20)\n                .map((word, index) => (\n                  <word\n                    key={index}\n                    style={{ width: (width / 100) * word.length }}\n                  />\n                ))}\n            </para>\n          )\n        })}\n      </minimap>\n    )\n  }\n\n  styles = {\n    minimap: {\n      display: \"block\",\n    },\n    hasComments: {\n      background: \"#eaf5fb\",\n    },\n    para: {\n      flexFlow: \"row\",\n      position: \"relative\",\n      flexWrap: \"wrap\",\n      padding: `3px 6px`,\n      lineHeight: 1,\n      transition: \"background 300ms cubic-bezier(0.785, 0.135, 0.150, 0.860)\",\n      borderLeft: `1px solid rgba(0, 0, 0, 0)`,\n    },\n    active: {\n      borderLeft: `1px solid rgba(0, 0, 0, 0.7)`,\n    },\n    liked: {\n      background: \"rgba(234, 70, 60, 0.1)\",\n      borderLeft: `1px solid rgba(255,109,94, 1)`,\n    },\n    word: {\n      height: 2,\n      marginLeft: 1,\n      marginRight: 1,\n      marginTop: 1,\n      marginBottom: 1,\n      background: `rgba(0, 0, 0, 0.2)`,\n    },\n  }\n}\n"},"lineCount":null}},"hash":"b83da0986430aa01e6c220347a4b8f07","cacheData":{"env":{}}}