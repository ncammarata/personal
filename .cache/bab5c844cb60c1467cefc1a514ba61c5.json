},"source":"app/views/chapter.js","original":{"line":63,"column":3}},{"name":"Component","generated":{"line":122,"column":2},"source":"app/views/chapter.js","original":{"line":14,"column":37}},{"name":"Chapter","generated":{"line":124,"column":18},"source":"app/views/chapter.js","original":{"line":14,"column":21}}],"sources":{"app/views/chapter.js":"import ReactHtmlParser, {\n  processNodes,\n  convertNodeToElement,\n  htmlparser2,\n} from \"react-html-parser\"\nimport { observer } from \"mobx-react\"\nimport Comments from \"./comments\"\n\n/*\n  <p>hello <i>to the</i> world</p>\n*/\n\n@observer\nexport default class Chapter extends Component {\n  show() {\n    const { html, store } = this.props\n    store.comments\n\n    const transform = node => {\n      if (\n        node.name === \"a\" &&\n        node.attribs &&\n        node.attribs.class === \"pageno\"\n      ) {\n        return (\n          <pageContainer number={node.attribs.id}>{node.data}</pageContainer>\n        )\n      }\n\n      if (node.name === \"p\") {\n        const text = node.children.map(child => child.data).join(\"\")\n        if (store.comments && text && store.comments[text.hashCode()]) {\n          const comment = store.comments[text.hashCode()]\n\n          const pieces = text.split(comment.selected)\n\n          return (\n            <p className=\"para\">\n              {pieces[0]}\n              <highlight style={{ display: \"inline\" }}>\n                {comment.selected}\n              </highlight>\n              {pieces[1]}\n              <Comments store={store} list={[comment]} />\n            </p>\n          )\n        }\n\n        if (!text || (text + \"\").trim().length === 0) {\n          return null\n        }\n\n        return <p className=\"para\">{text}</p>\n      }\n    }\n\n    const options = {\n      decodeEntities: true,\n      transform,\n    }\n\n    return <chapter>{ReactHtmlParser(html, options)}</chapter>\n  }\n\n  styles = {\n    img: {\n      display: \"none\",\n    },\n  }\n}\n"},"lineCount":null}},"hash":"a183acb8656c47969b0ba5f8b4fde926","cacheData":{"env":{}}}