ction\n                >\n                  delete\n                </delete>\n              </actions>\n            </comment>\n          ))}\n        </inner>\n      </comments>\n    )\n  }\n\n  styles = {\n    comments: {},\n    inner: {\n      position: \"relative\",\n      background: \"white\",\n    },\n    comment: {\n      boxShadow: `1px 1px 5px rgba(0, 0, 0, 0.1)`,\n      border: `1px solid rgba(0, 0, 0, 0.2)`,\n      padding: 8,\n      marginBottom: 20,\n      borderRadius: 5,\n      transition: \"all 300ms ease-in\",\n    },\n    active: {\n      transform: `scale(1.02)`,\n      boxShadow: `1px 1px 5px rgba(0, 0, 0, 0.2)`,\n      border: `1px solid rgba(0, 0, 0, 0.4)`,\n    },\n    row: {\n      flexFlow: \"row\",\n    },\n    actions: {\n      flexFlow: \"row\",\n      marginTop: 10,\n      alignItems: \"center\",\n    },\n    dot: {\n      fontSize: 12,\n    },\n    action: {\n      fontSize: 12,\n      cursor: \"pointer\",\n      color: \"#0070e0\",\n      fontWeight: 600,\n      paddingLeft: 3,\n      paddingRight: 3,\n    },\n    avatar: {\n      width: 35,\n      marginTop: 3,\n      marginLeft: 3,\n    },\n    text: {\n      marginLeft: 15,\n      fontSize: 16,\n    },\n  }\n}\n"},"lineCount":null}},"hash":"36534925537dd06c7e7ff79ef23dd35c","cacheData":{"env":{}}}